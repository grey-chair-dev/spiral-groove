{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/robots.txt", "destination": "/api/robots.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex,nofollow"
        }
      ],
      "has": [
        {
          "type": "host",
          "value": "spiralgrooverecords.greychair.io"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index,follow"
        }
      ],
      "has": [
        {
          "type": "host",
          "value": "spiralgrooverecords.com"
        }
      ]
    }
  ]
}


